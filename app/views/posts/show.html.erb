

<% current_user.posts.each do |post| %>
  <p>
    <h1>City: <%= post.city.name %></h1>
    <%= post.text %>

  </p>
    <%= link_to 'Edit Post', edit_user_post_path(current_user, post) %>
  <p><%= link_to "Delete this post", user_post_path(current_user, post), data: {confirm: "Are you sure you want to delet this post from #{post.city.name}"}, method: :delete %></p>

  <hr>
<% end %>
